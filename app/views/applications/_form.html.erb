<%= form_tag path, method: method do %>
  <section id="application"><%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <%= label_tag :description %>
  <%= text_field_tag :description %>
  <%= submit_tag button_text %>
  </section>
<% end %>
